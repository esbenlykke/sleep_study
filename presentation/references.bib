	pmcid = {PMC3184184},
	keywords = {Algorithms, Humans, Adult, Aged, Female, Male, Middle Aged, Young Adult, Acceleration, Motor Activity, Body Weight, Monitoring, Ambulatory, Sedentary Behavior, Sports Equipment},
	pages = {357--364},
