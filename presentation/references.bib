	year = {2015},
	pages = {4},
	file = {Hirshkowitz et al. - 2015 - National Sleep Foundation’s sleep time duration re.pdf:/home/esbenlykke/Zotero/storage/VCLU885M/Hirshkowitz et al. - 2015 - National Sleep Foundation’s sleep time duration re.pdf:application/pdf},
