	url = {https://ijbnpa.biomedcentral.com/articles/10.1186/1479-5868-10-120},
	doi = {10.1186/1479-5868-10-120},
	abstract = {Five accelerometer-derived methods of identifying nonwear and wear time were compared with a self-report criterion in adults ≥ 56 years of age. Two hundred participants who reported wearing an Actical™ activity monitor for four to seven consecutive days and provided complete daily log sheet data (i.e., the criterion) were included. Four variables were obtained from log sheets: 1) dates the device was worn; 2) time(s) the participant put the device on each day; 3) time(s) the participant removed the device each day; and 4) duration of self-reported nonwear each day. Estimates of wear and nonwear time using 60, 90, 120, 150 and 180 minutes of consecutive zeroes were compared to estimates derived from log sheets. Compared with the log sheet, mean daily wear time varied from -84, -43, -24, -14 and -8 min/day for the 60-min, 90-min, 120-min, 150-min and 180-min algorithms, respectively. Daily log sheets indicated 8.5 nonwear bouts per week with 120-min, 150-min and 180-min algorithms estimating 8.2-8.9 nonwear bouts per week. The 60-min and 90-min methods substantially overestimated number of nonwear bouts per week and underestimated time spent in sedentary behavior. Sensitivity (number of compliant days correctly identified as compliant) improved with increasing minutes of consecutive zero counts and stabilized at the 120-min algorithm. The proportion of wear time being sedentary and absolute and proportion of time spent in physical activity of varying intensities were nearly identical for each method. Utilization of at least 120 minutes of consecutive zero counts will provide dependable population-based estimates of wear and nonwear time, and time spent being sedentary and active in older adults wearing the Actical™ activity monitor.},
	language = {en},
	number = {1},
	urldate = {2022-04-13},
	journal = {International Journal of Behavioral Nutrition and Physical Activity},
	author = {Hutto, Brent and Howard, Virginia J. and Blair, Steven N. and Colabianchi, Natalie and Vena, John E. and Rhodes, David and Hooker, Steven P.},
	month = dec,
	year = {2013},
	note = {Number: 1
Publisher: BioMed Central},
	pages = {1--8},
	file = {Full Text PDF:/home/esbenlykke/Zotero/storage/9JB6MLBI/Hutto et al. - 2013 - Identifying accelerometer nonwear and wear time in.pdf:application/pdf;Snapshot:/home/esbenlykke/Zotero/storage/GXY4IHB7/1479-5868-10-120.html:text/html},
}

@article{kidholm_model_2012,
	title = {A {Model} for {Assessment} of {Telemedicine} {Applications}: {Mast}},
	volume = {28},
	issn = {1471-6348},
	shorttitle = {A {Model} for {Assessment} of {Telemedicine} {Applications}},
